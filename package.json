    "dev": "backpack dev",
    "build": "backpack build",
    "test": "jest"
  },
  "license": "MIT",
  "dependencies": {
    "1password": "^0.4.0",
    "@types/node": "^9.4.6",
    "@types/node-forge": "^0.7.2",
    "chalk": "^2.3.0",
    "dotenv": "^4.0.0",
    "lodash": "^4.17.5",
    "node-fetch": "^2.1.1",
    "node-forge": "^0.7.2",
    "p-queue": "^2.3.0",
    "shallowequal": "^1.0.2",
    "timeout-as-promise": "^1.0.0",
    "ws": "^4.1.0"
  },
  "devDependencies": {
    "@types/chalk": "^2.2.0",
    "@types/dotenv": "^4.0.2",
    "@types/lodash": "^4.14.104",
    "@types/node-fetch": "^1.6.7",
    "@types/p-queue": "^2.3.1",
    "@types/shallowequal": "^0.2.1",
    "@types/ws": "^4.0.1",
    "awesome-typescript-loader": "^3.4.1",
    "backpack-core": "^0.4.3",
    "jest": "^22.0.0",
    "ts-jest": "^21.2.4",
    "tslint": "^5.8.0",
    "tslint-eslint-rules": "^4.1.1",
    "tslint-react": "^3.2.0",
    "typescript": "^2.6.2"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "setupFiles": [
      "./src/setupTests.ts"
    ],
    "testEnvironment": "node",
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx)$"
    ],
    "globals": {
      "__DEV__": true
    }
  }
}
